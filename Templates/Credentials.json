{
    "postgresql":{
        "connection_string":"jdbc:postgresql://localhost:5432/{database}",
        "driver":"org.postgresql.Driver"
    },
    "mysql":{
        "connection_string":"jdbc:mysql://localhost:3306/{database}",
        "driver":"com.mysql.cj.jdbc.Driver"
    },
    "oracle":{
        "connection_string":"jdbc:oracle:thin:@localhost:1521/{database}",
        "driver":"oracle.jdbc.OracleDriver"
    },
    "sqlserver":{
        "connection_string":"jdbc:sqlserver://localhost:1433;databaseName={database}",
        "driver":"com.microsoft.sqlserver.jdbc.SQLServerDriver"
    }
}
